(($) ->
    $.ender
        ajaj: (options) ->
            options.method or= 'GET'
            options.type = 'json'
            options.headers or= {}
            options.headers['Content-Type'] = 'application/json'
            
            if options.data
                options.data = JSON.stringify(options.data)
            
            return $.ajax(options)

)(ender)